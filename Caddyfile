localhost breccia-mapper-dev breccia-mapper-dev.jagraham.co.uk mapper.gcrf-breccia.com {

    tls internal

    root * /srv
    file_server

    @proxy_paths {
        not path /static/*
    }

    reverse_proxy @proxy_paths http://web:8000

    log {
        output stderr
        format single_field common_log
    }
}